<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
     <loc>https://www.sliplife.com/</loc>
     <lastmod>
       {@formatDate val=now year="numeric"/}-{@formatDate val=now day="2-digit"/}-{@formatDate val=now month="2-digit"/}
     </lastmod>
     <changefreq>daily</changefreq>
     <priority>0.9</priority>
  </url>
  {#listings}
     <url>
        <loc>https://www.sliplife.com/listings/{id}</loc>
        <lastmod>
          {@formatDate val=createdAt year="numeric"/}-{@formatDate val=createdAt day="2-digit"/}-{@formatDate val=createdAt month="2-digit"/}
        </lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
     </url>
   {/listings}
</urlset>
